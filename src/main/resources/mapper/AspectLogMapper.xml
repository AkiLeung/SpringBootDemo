<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.demo.dao.IAspectLogDao">

    <!-- 新增 -->
    <insert id="saveAspectLog">
        insert into tbl_aspect_log
        (uuid,username,operation,time,method,params,ip,create_time) values
        (#{uuid},#{username},#{operation},#{time},#{method},#{params},#{ip},#{createTime})
    </insert >

</mapper>
